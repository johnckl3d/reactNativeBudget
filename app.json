{
  "expo": {
    "name": "Budget Tracker",
    "slug": "budget-tracker",
    "privacy": "public",
    "platforms": [
      "ios"
    ],
    "version": "^45.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.johncheang.budget",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.johncheang.budget"
    }
  }
}
